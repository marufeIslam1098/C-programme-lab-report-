#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    printf("Enter a string: ");
    scanf("%s", str);

    char *left = str;
    char *right = str + strlen(str) - 1;

    int flag = 1;

    while(left < right) {
        if(*left != *right) {
            flag = 0;
            break;
        }
        left++;
        right--;
    }

    if(flag) printf("Palindrome\n");
    else     printf("Not Palindrome\n");

    return 0;
}

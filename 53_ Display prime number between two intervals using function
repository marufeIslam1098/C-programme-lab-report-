#include <stdio.h>
#include <math.h>
int isPrime(int x){
    if (x<=1) return 0;
    if (x==2) return 1;
    if (x%2==0) return 0;
    for (int i=3;i*i<=x;i+=2) if (x%i==0) return 0;
    return 1;
}
int main(){
    int a,b; printf("Enter lower and upper: "); scanf("%d%d",&a,&b);
    if (a>b){ int t=a;a=b;b=t; }
    printf("Primes between %d and %d:\n", a, b);
    for (int i=a;i<=b;i++) if (isPrime(i)) printf("%d ", i);
    printf("\n");
    return 0;
}

#include <stdio.h>

int main(){
    int n; printf("Enter rows: "); scanf("%d",&n);
    char c='A';
    for (int i=1;i<=n;i++){
        for (int j=0;j<i;j++){
            putchar(c);
            c = (c=='Z') ? 'A' : c+1;
            putchar(' ');
        }
        putchar('\n');
    }
    return 0;
}

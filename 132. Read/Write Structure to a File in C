#include <stdio.h>

struct Student {
    char name[20];
    int age;
};

int main() {
    struct Student s1 = {"Rafi", 20};
    FILE *fp = fopen("data.bin", "wb");
    fwrite(&s1, sizeof(s1), 1, fp);
    fclose(fp);

    struct Student s2;
    fp = fopen("data.bin", "rb");
    fread(&s2, sizeof(s2), 1, fp);
    fclose(fp);

    printf("Name: %s Age: %d\n", s2.name, s2.age);
    return 0;
}

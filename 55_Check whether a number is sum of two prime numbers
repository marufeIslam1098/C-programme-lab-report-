#include <stdio.h>
#include <math.h>

int isPrime(int x){
    if (x<=1) return 0;
    if (x==2) return 1;
    if (x%2==0) return 0;
    for (int i=3;i*i<=x;i+=2) if (x%i==0) return 0;
    return 1;
}

int main(){
    int n; printf("Enter number: "); scanf("%d",&n);
    int found=0;
    for (int i=2;i<=n/2;i++){
        if (isPrime(i) && isPrime(n-i)){
            printf("%d = %d + %d\n", n, i, n-i);
            found=1;
        }
    }
    if (!found) printf("%d cannot be expressed as sum of two primes\n", n);
    return 0;
}

#include <stdio.h>
#include <math.h>
int isPrime(int x){
    if (x<=1) return 0;
    if (x==2) return 1;
    if (x%2==0) return 0;
    for (int i=3;i*i<=x;i+=2) if (x%i==0) return 0;
    return 1;
}
int main(){
    int n; printf("Enter number: "); scanf("%d",&n);
    printf("%d is %s\n", n, isPrime(n) ? "prime" : "not prime");
    return 0;
}
